  },
  "homepage": "https://github.com/IrisVR/iris-error-handler#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chalk": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.14.0",
    "ghooks": "^2.0.0",
    "mocha": "^3.2.0",
    "mockgoose": "^6.0.8",
    "mongoose": "^4.7.6",
