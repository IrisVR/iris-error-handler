{
  "name": "iris-error-handler",
  "version": "0.0.0-semantically-released",
  "description": "Resource for internal Iris error codes and middleware",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "rsync -av --exclude 'specs' src/ dist",
    "test": "node_modules/.bin/_mocha src/specs/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "lint": "./node_modules/.bin/eslint ./src",
    "coverage": "nyc npm test",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel lint coverage",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IrisVR/iris-error-handler.git"
  },
  "files": [
    "dist"
  ],
  "author": "robin@irisvr.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IrisVR/iris-error-handler/issues"
  },
  "homepage": "https://github.com/IrisVR/iris-error-handler#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chalk": "^1.1.3",
    "codecov": "^1.0.1",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.14.0",
    "ghooks": "^2.0.0",
    "mocha": "^3.2.0",
    "mockgoose": "^6.0.8",
    "mongoose": "^4.7.6",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "should": "^11.1.2",
    "supertest": "^2.0.1",
    "validate-commit-msg": "^2.8.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "validate-commit-msg"
    }
  }
}
